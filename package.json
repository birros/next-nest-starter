{
  "name": "next-nest-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate && next build",
    "start": "next start",
    "generate": "npm run generate:prisma && npm run generate:api",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "generate:prisma": "prisma generate",
    "migrate": "prisma migrate dev",
    "seed": "prisma db seed --preview-feature",
    "studio": "prisma studio",
    "generate:api": "npm run api:yaml && npm run api:client && npm run api:fix-basepath",
    "api:yaml": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --files scripts/openapi.ts .openapi.yaml",
    "api:client": "openapi-generator-cli generate -i .openapi.yaml -g typescript-axios -o lib/api",
    "api:fix-basepath": "sed -i.bak 's/export const BASE_PATH = .*/export const BASE_PATH = \"\"/g' lib/api/base.ts"
  },
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/swagger": "^4.8.0",
    "@prisma/client": "^2.22.1",
    "class-transformer": "^0.4.0",
    "connect-redis": "^5.2.0",
    "express-session": "^1.17.1",
    "immer": "^9.0.2",
    "lodash": "^4.17.21",
    "next": "^10.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.16.0",
    "redis": "^3.1.2",
    "secure-password": "^4.0.0",
    "session-file-store": "^1.5.0",
    "swagger-ui-express": "^4.1.6",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@nestjs/platform-express": "^7.6.15",
    "@openapitools/openapi-generator-cli": "^2.2.6",
    "@types/express-session": "^1.17.3",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.5",
    "@types/redis": "^2.8.28",
    "@types/secure-password": "^3.1.0",
    "@types/yup": "^0.29.11",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "prisma": "^2.22.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "yaml": "^1.10.2"
  }
}
